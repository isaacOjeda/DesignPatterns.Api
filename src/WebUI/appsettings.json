{
  "UseInMemoryDatabase": false,
  "ConnectionStrings": {
    "DefaultConnection": "Server=(localdb)\\mssqllocaldb;Database=DesignPatterns.ApiDb;Trusted_Connection=True;MultipleActiveResultSets=true;"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "IdentityServer": {
    "Clients": {
      "DesignPatterns.Angular.WebUI": {
        "Profile": "SPA",
        "RedirectUri": "https://localhost:5003/authentication/login-callback",
        "LogoutUri": "https://localhost:5003/authentication/logout-callback"
      }
    }
  },
  "AllowedHosts": "*"
}
